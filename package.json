{
  "name": "fx-style",
  "version": "2.1.2",
  "description": "Felix Tellmann TypeScript Style",
  "repository": "FelixTellmann/fx-style",
  "main": "build/src/index.js",
  "bin": {
    "gts": "build/src/cli.js"
  },
  "files": [
    "CHANGELOG.md",
    "build/src",
    "build/template",
    "build/.eslintrc.js",
    ".prettierrc.json",
    "tsconfig-google.json",
    "tsconfig.json",
    ".eslintrc.js"
  ],
  "scripts": {
    "build": "npm run create:eslint && npm run compile",
    "clean": "rimraf ./build/",
    "create:eslint": "copyfiles \".eslintrc.js\" \"build/\"",
    "compile": "tsc",
    "postcompile": "ncp template build/template",
    "lint": "eslint '**/*.ts'",
    "prepare": "npm run build",
    "test": "c8 mocha build/test/test-*.js",
    "system-test": "c8 mocha build/test/kitchen.js",
    "pretest": "npm run build",
    "presystem-test": "npm run build",
    "license-check": "jsgl --local .",
    "fix": "eslint --fix '**/*.ts'"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "typescript",
    "linter",
    "formatter",
    "eslint",
    "prettier"
  ],
  "author": "Felix Tellmann",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "8.43.0",
    "eslint-config-next": "^13.4.6",
    "eslint-plugin-react": "^7.32.2",
    "typescript": "^5.1.3",
    "chalk": "^4.1.0",
    "copyfiles": "^2.4.1",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-prettier-fx": "^3.4.2",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "execa": "^5.0.0",
    "inquirer": "^7.3.3",
    "json5": "^2.2.1",
    "meow": "^9.0.0",
    "ncp": "^2.0.0",
    "prettier": "^2.7.1",
    "prettier-fx": "^2.6.3-dev",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "write-file-atomic": "^3.0.3"
  },
  "devDependencies": {
    "@types/eslint": "^8.40.2",
    "@types/node": "^20.3.1",
    "@npm/types": "^1.0.2",
    "@types/cross-spawn": "^6.0.2",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "8.2.1",
    "@types/json5": "2.2.0",
    "@types/mocha": "^9.1.1",
    "@types/ncp": "^2.0.5",
    "@types/prettier": "^2.7.0",
    "@types/rimraf": "^3.0.2",
    "@types/sinon": "^10.0.13",
    "@types/tmp": "^0.2.3",
    "@types/write-file-atomic": "^4.0.0",
    "c8": "^7.12.0",
    "cross-spawn": "^7.0.3",
    "fs-extra": "^10.1.0",
    "inline-fixtures": "^1.1.0",
    "js-green-licenses": "^3.0.1",
    "mocha": "^9.2.2",
    "sinon": "^13.0.1",
    "tmp": "0.2.1"
  }
}
