{
  "name": "fx-style",
  "version": "1.3.0",
  "description": "Felix Tellmann TypeScript Style",
  "repository": "FelixTellmann/fx-style",
  "main": "build/src/index.js",
  "bin": {
    "gts": "build/src/cli.js"
  },
  "files": [
    "CHANGELOG.md",
    "build/src",
    "build/template",
    "build/.eslintrc.js",
    ".prettierrc.json",
    "tsconfig-google.json",
    "tsconfig.json",
    ".eslintrc.js"
  ],
  "scripts": {
    "build": "npm run create:eslint && npm run compile",
    "clean": "rimraf ./build/",
    "create:eslint": "copyfiles \".eslintrc.js\" \"build/\" && copyfiles \"tailwindcss-custom-groups.js\" \"build/\"",
    "compile": "tsc",
    "postcompile": "ncp template build/template",
    "lint": "eslint '**/*.ts'",
    "prepare": "npm run build",
    "test": "c8 mocha build/test/test-*.js",
    "system-test": "c8 mocha build/test/kitchen.js",
    "pretest": "npm run build",
    "presystem-test": "npm run build",
    "license-check": "jsgl --local .",
    "fix": "eslint --fix '**/*.ts'"
  },
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "typescript",
    "linter",
    "formatter",
    "eslint",
    "prettier"
  ],
  "author": "Felix Tellmann",
  "license": "MIT",
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "chalk": "^4.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.10.0",
    "eslint-config-next": "^12.0.4",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-prettier-fx": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "eslint-plugin-tailwindcss": "^1.8.0",
    "eslint-plugin-typescript-sort-keys": "^1.6.0",
    "execa": "^5.0.0",
    "inquirer": "^7.3.3",
    "json5": "^2.1.3",
    "meow": "^9.0.0",
    "ncp": "^2.0.0",
    "next": "^12.0.4",
    "prettier": "^2.5.0",
    "prettier-fx": "^2.4.10-dev",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5",
    "update-notifier": "^5.0.0",
    "write-file-atomic": "^3.0.3"
  },
  "devDependencies": {
    "@npm/types": "^1.0.1",
    "@types/cross-spawn": "^6.0.2",
    "@types/eslint": "^7.2.3",
    "@types/fs-extra": "^9.0.1",
    "@types/inquirer": "^7.3.1",
    "@types/json5": "0.0.30",
    "@types/mocha": "^8.0.3",
    "@types/ncp": "^2.0.4",
    "@types/node": "^14.11.2",
    "@types/prettier": "^2.1.1",
    "@types/rimraf": "^3.0.0",
    "@types/sinon": "^10.0.0",
    "@types/tmp": "^0.2.0",
    "@types/update-notifier": "^5.0.0",
    "@types/write-file-atomic": "^3.0.1",
    "c8": "^7.3.1",
    "cross-spawn": "^7.0.3",
    "fs-extra": "^9.0.1",
    "inline-fixtures": "^1.1.0",
    "js-green-licenses": "^3.0.0",
    "mocha": "^8.1.3",
    "sinon": "^10.0.0",
    "tmp": "0.2.1"
  },
  "peerDependencies": {
    "typescript": ">=3"
  }
}
